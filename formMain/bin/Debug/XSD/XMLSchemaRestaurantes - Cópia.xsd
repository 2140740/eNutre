<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified"
           elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:element name="restaurantes" type="restaurantesType"/>
  <xs:complexType name="restauranteType">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element type="xs:string" name="nome"/>
      <xs:element type="xs:string" name="item"/>
      <xs:element type="xs:string" name="quantidade"/>
      <xs:element type="xs:integer" name="calorias"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="restaurantesType">
    <xs:sequence>
      <xs:element type="restauranteType" name="restaurante" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
